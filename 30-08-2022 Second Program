#include <stdio.h>  

int main()  

{  

    int i,len=0,j=2;  

    char str[200];  

    scanf("%[^\n]s",&str);

    len = sizeof(str)/sizeof(str[0]);   

    for(i=0;i<len;i++)

    {

        if(str[i]=='a')

        {

            str[i] = toupper(str[i]);  

        }

    } 

    printf("\n"); 

    printf("After changing %s", str);  

    return 0;  

}  
